{% comment %}
board_data: <strong>{{ board_data }}</strong>
{% endcomment %}
<table cellspacing=1 width=700 border=1>
    <tr>
        <td width=50><p align=center>번호</p>
        </td>
        <td width=200><p align=center>{{ board_data.id }}</p>
        </td>
    </tr>
	<tr>
        <td width=50><p align=center>이름</p>
        </td>
        <td width=200><p align=center>{{ board_data.user_name }}</p>
        </td>
    </tr>
	<tr>
        <td width=50><p align=center>제목</p>
        </td>
        <td width=200><p align=center>{{ board_data.board_title }}</p>
        </td>
    </tr>
	<tr>
        <td width=50><p align=center>내용</p>
        </td>
        <td width=200><p align=center>{{ board_data.board_content }}</p>
        </td>
    </tr>
	<tr>
        <td width=50><p align=center>조회수</p>
        </td>
        <td width=200><p align=center>{{ board_data.count }}</p>
        </td>
    </tr>
</table>